@{
    ViewData["Title"] = "Usuarios";
}

@model List<Domain.Entities.IM253E01Usuario>

<a asp-action="Create">Crear Nuevo</a>

<h1>
    Mostrar:
</h1>

<table>
    <tr>
        <th>Nombre</th>
        <th>Edad</th>
        <th></th>
    </tr>
    @foreach (var e in Model)
    {
        <tr>
            <th>@e.Nombre</th>
            <td>@e.Edad</td>
            <td>
                <a asp-action="Details" asp-route-id="@e.Id">Detalles</a> |
                <a asp-action="Edit" asp-route-id="@e.Id">Editar</a> |
                <a asp-action="Delete" asp-route-id="@e.Id">Eliminar</a>
            </td>
        </tr>
    }
</table>